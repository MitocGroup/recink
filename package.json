    "github-slug": "^2.0.0",
    "parse-github-repo-url": "^1.4.0"
  },
  "devDependencies": {
    "dependo": "^0.1.6",
    "esdoc": "^0.5.2",
    "esdoc-node": "^1.0.2"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "preferGlobal": true
