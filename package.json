    "global-modules": "^1.0.0",
    "istanbul": "^0.4.5",
    "jag": "^2.0.1",
    "md5-file": "^3.1.1",
    "md5-hex": "^2.0.0",
    "merge": "^1.2.0",
    "mocha": "^5.0.3",
    "ora": "^1.2.0",
    "package-hash": "^2.0.0",
    "pify": "^3.0.0",
    "pretty-bytes": "^4.0.2",
    "print": "^1.0.2",
    "progress-stream": "^2.0.0",
    "readdir-enhanced": "^1.5.2",
    "resolve-package": "^1.0.1",
